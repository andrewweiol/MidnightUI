    color: #ffffff;
    background-color: #D57346;
}

QToolBar > QPushButton:checked:pressed {
    background-color: solid #D57346;
}

QToolBar > QPushButton:!checked:pressed {
    background-color: #D57346;
}

QToolBar > QPushButton:checked:disabled,
QToolBar > QPushButton:!checked:disabled {
    border: none;
    background-color: transparent;
}

QToolBar > QToolButton {
    max-width: 50px;
    min-width: 24px;
    max-height: 50px; 
    background-color: rgba(255,255,255,0.05); 
    margin: 0px 1px 2px 1px;
    padding: 3px;
    border-radius: 3px;
}

QToolBar > QToolButton:hover {
    background-color: #594FF7;
    border: 0px solid #E6E6E6;
}

QToolBar > QToolButton:pressed {
    background-color: #D57346;
    border: 0px solid #D57346;
}

/* ToolBar menu buttons (buttons with drop-down menu) */
QToolBar > QToolButton#qt_toolbutton_menubutton {
    max-width: 50px;
    min-width: 24px; 
    max-height: 50px; 
    border-radius: 3px; 
    padding: 3px;
    margin: 0px 1px 2px 1px;
    background-color: rgba(255,255,255,0.05);
}

QToolBar QToolButton > QMenu::item {
    padding: 10px 20px 10px 20px;
}

QToolBar QToolButton > QMenu::icon {
    padding-left: 15px;
}

QToolBar QToolButton::menu-button,
QToolBar > QToolButton#qt_toolbutton_menubutton::menu-button {
    subcontrol-origin: padding; 
    subcontrol-position: bottom left;
    border: none;
    width: 15px;
    height: 15px;
    outline: none;
    margin-left: 1px;
    border-radius: 3px;
    background-color: transparent;
}

QToolBar > QToolButton#qt_toolbutton_menubutton::menu-button:hover {
    /* VertUI, Remove: border-bottom: 1px solid #E6E6E6; */
    background-color: rgba(0,0,0,0.5);
}

QToolBar > QToolButton#qt_toolbutton_menubutton:pressed,
QToolBar > QToolButton#qt_toolbutton_menubutton:open {
    background-color: #D57346;
    border: 0px solid #D57346; /* Vert UI, Default: 1px; */
}

QToolBar > QToolButton#qt_toolbutton_menubutton:hover {
    background-color: #D57346;
    border: 0px solid #E6E6E6; /* Vert UI, Default: 1px; */
}

QToolBar > QToolButton#qt_toolbutton_menubutton::menu-button:pressed,
QToolBar > QToolButton#qt_toolbutton_menubutton::menu-button:open {
}

QToolBar > QToolButton::menu-arrow {
    background-image: url(qss:images_dark-light/expand_arrow.svg); /* VertUI, Default: background-image: url(qss:images_dark-light/down_arrow_lighter.svg); */
    background-position: center center;
    background-repeat: none;
    subcontrol-origin: padding;
    subcontrol-position: bottom left; /* VertUI, Default: subcontrol-position: bottom right; */
    height: 10px; /* same as arrow image */
}

QToolBar > QToolButton::menu-arrow:hover {
    background-image: url(qss:images_dark-light/expand_arrow.svg); /* VertUI, Default: background-image: url(qss:images_dark-light/down_arrow_lighter.svg); */
}

QToolBar > QToolButton::menu-arrow:open {
    background-image: url(qss:images_dark-light/expand_arrow.svg); /* VertUI, Default: background-image: url(qss:images_dark-light/down_arrow_lighter.svg); */
}

/* when QToolButton is checked: */
QToolBar > QToolButton:checked {
    border: 0px solid #D57346; /* VertUI */
    background-color: #484848; /* VertUI, Default: background-color: #D57346; */
}

QToolBar > QToolButton:checked:hover {
    border: 0px solid #D57346;
    background-color: #D57346; /* transparency for #D57346 color */
}

/*The "show more" button  (it can also be stylable with "QToolBarExtension" */
