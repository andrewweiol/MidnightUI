    image: none;
    width: 0px;
}

QToolBar > QPushButton:!checked {
    background-color: #1E1E26; /* Current Working Plane and Nudge */
    padding: 2px 4px;
    margin: 0px 2px;
}

QToolBar > QPushButton:checked:hover {
}

