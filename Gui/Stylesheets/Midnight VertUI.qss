/*==================================================================================================
Reset elements
==================================================================================================*/
/* Resetting everything helps to unify styles across different operating systems */
* {
    padding: 0px;
    margin: 0px;
    border: 0px;
    border-style: none;
    border-image: none;
    outline: none;
    color: #E6E6E6; /* Default color for labels and different text elements that usually use dark colors */
}

/* specific reset for elements inside QToolBar */
QToolBar * {
    margin: 0px;
    padding: 0px;
}

/*==================================================================================================
Style Links
==================================================================================================*/
QLabel[haslink="true"] {
    color: #594FF7;
}

Gui--UrlLabel {
    color: #594FF7;
}

/*==================================================================================================
Main window
==================================================================================================*/
QWidget {
    background-color: transparent;
}

QMainWindow,
QDialog,
QDockWidget,
QToolBar  {
    background-color: #1E1E26;
}

QMdiArea {
    background-image: url(qss:images_dark-light/background_freecad_dark.svg);
    background-position: center center;
    background-repeat: no-repeat;
}

ExpressionLabel {
    background-color: transparent;
    margin-top: 1px;
}

/*==================================================================================================
Sub windows
==================================================================================================*/
QMdiSubWindow {
    border:1px solid #1E1E26;
    margin-top: 30px;    
    margin-left: 4px;    
    margin-right: 4px;
    margin-bottom:4px;
}

/*==================================================================================================
QToolBox
==================================================================================================*/
QToolBox {
	background-color: transparent;
}

QToolBox::tab {
	background-color: #27282E;
	border: 2x solid #34353c;
    border-radius: 2px;
}

QToolBox::tab:hover {
	background-color: #D57346;
	border: 1px solid #594FF7;
    border-radius: 1px
}

/*==================================================================================================
QStatusBar
==================================================================================================*/
QStatusBar > QLabel {
    margin-left: 4px;
}

QStatusBar::item {
    border: 1px solid #1E1E26;
    border-radius: 0px;
}

/*==================================================================================================
MENUS
==================================================================================================*/
QMenuBar,
QMenuBar::item {
    color: #E6E6E6;
    background-color: #1E1E26; /* top menu main background color */
}

QMenu,
QMenu::item {
    color: #E6E6E6;
    background-color: #27282E;
    text-decoration: none;
}

QMenu::item {
    padding: 4px 16px 4px 8px; 
    border: 1px solid transparent; /* reserve space for selection border */
}

QMenuBar::item:selected,
QMenuBar::item:pressed,
QMenu::item:selected,
QMenu::item:pressed {
    color: #FFF;
    background-color: #D57346;
    border-radius: 4px;
}

QMenu::right-arrow {
    width: 10px;
    height: 10px;
    image:url(qss:images_dark-light/right_arrow_light.svg);
    margin-right: 2px;
}

QMenu::right-arrow:selected {
    image:url(qss:images_dark-light/right_arrow_lighter.svg);
}

QMenu::icon {
    margin-left: 8px;
}

QMenu::separator {
    height: 1px;
    background-color: rgba(255,255,255,30);
    margin: 6px 4px;
}

QMenu::indicator:non-exclusive:checked {
    color: #E6E6E6;
}

/* Fix for elements inside a drop-down menu */
QMenu QRadioButton,
QMenu QCheckBox,
QMenu QPushButton,
QMenu QToolButton {
    color: #E6E6E6; /* same as regular QRadioButton and QCheckBox */
    padding-top: 2px;
    padding-left: 2px;
}

QMenu QRadioButton:hover,
QMenu QCheckBox:hover,
QMenu QPushButton:hover,
QMenu QToolButton:hover,
QMenu QPushButton:pressed,
QMenu QToolButton:pressed,
QMenu QPushButton:selected,
QMenu QToolButton:selected {
    color: #E6E6E6;
    background-color: #D57346; /* same as QMenu::item:selected and QMenu::item:pressed */
}

QMenu QRadioButton:disabled,
QMenu QCheckBox:disabled {
    color: #E6E6E6;
}

QMenu QRadioButton::indicator:disabled,
QMenu QCheckBox::indicator:disabled {
    color: #E6E6E6;
    background-color: transparent;
    border: 1px solid #E6E6E6;
}

/*==================================================================================================
Tool bar
==================================================================================================*/
QToolBar {
}

QToolBar::handle:top,
QToolBar::handle:bottom,
QToolBar::handle:horizontal {
    background-image: url(qss:images_dark-light/Hmovetoolbar_light.svg);
    width: 10px;
    margin: 4px 2px;
    background-position: top right;
    background-repeat: repeat-y;
}

QToolBar::handle:left,
QToolBar::handle:right,
QToolBar::handle:vertical {
    background-image: url(qss:images_dark-light/Vmovetoolbar_light.svg);
    height: 10px;
    margin: 2px 4px;
    background-position: left bottom;
    background-repeat: repeat-x;
}

QToolBar::separator:top,
QToolBar::separator:bottom,
QToolBar::separator:horizontal {
    width: 0px;
}

QToolBar::separator:left,
QToolBar::separator:right,
QToolBar::separator:vertical {
}

QToolBarSeparator {
    max-width: 0px;
    max-height: 0px;
    margin: 0;
    padding: 0;
}

/*==================================================================================================
Group box
==================================================================================================*/
QGroupBox {
    color: #676773;
    border-left:1px solid rgba(46, 46, 59, 1.0);
    text-transform: uppercase;
    letter-spacing: 2px;
    margin-top: 20px;
    margin-left: 0px;
    padding-left: 0px;
}

QGroupBox:title {
    background-color: transparent;
    subcontrol-origin: margin;
    subcontrol-position: left top;
    padding-bottom: 2px;
    border-bottom: 1px solid rgba(46, 46, 59, 1.0);
    top: 4px;
    left: -3px;
}

/* NOTE: QGroupBox checkboxes are styles with regular ones so that the all get the same style */
/* QGroupBox::indicator {
    width: 13px;
    height: 13px;
}

QGroupBox::indicator:unchecked {
    image: url(:/images/checkbox_unchecked.png);
} */


/*==================================================================================================
Tooltip
==================================================================================================*/
QToolTip {
    color: #E6E6E6;
    background-color: #27282E;
    padding: 10px;
}

/*==================================================================================================
Dock widget (Non-overlay)
==================================================================================================*/
QDockWidget {
    titlebar-close-icon: url(qss:images_dark-light/close_light.svg);
    titlebar-normal-icon: url(qss:images_dark-light/undock_light.svg);
}

QDockWidget > Gui--OverlayTitleBar { /* OPAQUE DOCK TITLES*/
    color: #676773;
    text-transform: uppercase;
    letter-spacing: 2px;
}

QDockWidget > Gui--OverlayTitleBar > Gui--OverlayToolButton:hover {
    background-color: #D57346;
    border-radius: 4px;
}

/* Python Console */
QDockWidget > QFrame {
    background-color: #14141A;
    border: 0px solid;
    border-radius: 8px;
}

/*==================================================================================================
Progress bar
==================================================================================================*/
QProgressBar,
QProgressBar:horizontal {
    color: #E6E6E6;
    min-height: 24px;
    background-color: transparent;
    text-align: center;
    border: 0px solid rgba(0,0,0,140);
    padding: 1px;
    border-radius: 1px;
}
QProgressBar::chunk,
QProgressBar::chunk:horizontal {
    background-color: #D57346;
    border-radius: 1px;
}

/*==================================================================================================
Scroll
==================================================================================================*/
QAbstractScrollArea {
    border-radius: 1px;
    background-color: transparent;
}

QAbstractScrollArea::corner {
    border: none;
    background-color: transparent;
}

QScrollBar:horizontal {
    background-color: transparent;
    height: 14px;
    margin: 2px;
}

QScrollBar::handle:vertical,
QScrollBar::handle:horizontal {
    background-color: #1E1E26;
    border-radius: 5px;
    min-width: 5px;
    margin: 0px;
}

QScrollBar::handle:vertical:hover,
QScrollBar::handle:horizontal:hover {
    background-color: #D57346;
}

QScrollBar::sub-line:horizontal {
    background-color: transparent;
    border-radius: 5px;
    width: 10px;
    height: 10px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal {
    background-color: transparent;
    border-radius: 5px;
    width: 10px;
    height: 10px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:hover,
QScrollBar::sub-line:horizontal:on {
    background-color: #D57346;
}

QScrollBar::add-line:horizontal:hover,
QScrollBar::add-line:horizontal:on {
    background-color: #D57346;
}

QScrollBar::up-arrow:horizontal,
QScrollBar::down-arrow:horizontal {
    background-color: none;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background-color: transparent;
}

QScrollBar:vertical {
    background-color: transparent;
    width: 14px;
    margin: 2px;
    border-radius: 5px;
}

QScrollBar::handle:vertical {
    min-height: 24px;
    border-radius: 5px;
}

QScrollBar::sub-line:vertical {
    background-color: transparent;
    border-radius: 5px;
    width: 10px;
    height: 10px;
}

QScrollBar::add-line:vertical {
    background-color: transparent;
    border-radius: 5px;
    width: 10px;
    height: 10px;
}

QScrollBar::sub-line:vertical:hover,
QScrollBar::sub-line:vertical:on {
    background-color: #D57346;
}

QScrollBar::add-line:vertical:hover,
QScrollBar::add-line:vertical:on {
    background-color: #D57346;
}

QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical {
    background-color: none;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background-color: transparent;
}


/*==================================================================================================
Tab bar
==================================================================================================*/
QTabWidget::pane {
    background-color: transparent; /* temporal (transparent background) */ /* tab content background color */ /* was transparent. fixes no color undocked Combo View */
    position: absolute;
}

QTabWidget::pane:top {
    top: -1px;
    border-top: 1px solid #1E1E26;
}

QTabWidget::pane:bottom {
    bottom: -1px;
    border-bottom: 1px solid #33333C;
}

QTabWidget::pane:left {
    right: -1px;
    border-right: 1px solid #1E1E26;
}

QTabWidget::pane:right {
    left: -1px;
    border-left: 1px solid #1E1E26;
}

QTabWidget::tab-bar:top,
QTabWidget::tab-bar:bottom {
    left: 8px;
}

QTabWidget::tab-bar:left,
QTabWidget::tab-bar:right {
    top: 6px;
}

/* ========== WORKBENCH TABS ========== */

Gui--WorkbenchTabBar QTabBar::tab:top:selected {
    background-color: #34353c; /* SAME AS VIEWPORT! */
    qproperty-drawBase: 0; /* important */
}

Gui--WorkbenchTabBar {
    max-height: 32px;
}

/* == FreeCAD Link 07.04 == */

Gui--WorkbenchTabWidget QTabBar::tab:top:selected {
    background-color: #34353c; /* SAME AS VIEWPORT! */
    qproperty-drawBase: 0; /* important */    
}

Gui--WorkbenchTabWidget {
    max-height: 32px;
}

Gui--WorkbenchTabBar { 
    qproperty-tabSize: 22;
}

/* ========== OTHER TABS ========== */
QTabBar {
    qproperty-drawBase: 0; /* important */
    background-color: transparent; /* Hack for Undocked #E6E6E6 background - was transparent*/ 
}

QTabBar::tab { /* TABS MAIN */
    background-color: #27282E; /* Non-active normal tab */
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 5px;
    padding-bottom: 5px;
    margin-right: 1px;
    border-style: solid;
    border-color: transparent;
    min-width: 30px

}

QTabBar::tab:top {
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    border-style: solid;
    border-color: transparent;
    border-top-width: 2px;
    qproperty-iconSize: 20px;
}

QTabBar::tab:top:selected {
    background-color: #1e1e26;
    border-color: #D57346;
    border-radius: 0px;
}

QTabBar::tab:bottom {
    border-bottom-left-radius: 8px;
    border-bottom-right-radius: 8px;
    border-top-width: 2px; /* same as selected tab colored border in order to center close-button */
    border-bottom-width: 2px; /* same as selected tab colored border in order to center close-button */
    min-width: 11ex;
    padding-top: 2px;
    padding-bottom: 2px;
}

QTabBar::tab:bottom:selected {
    border-bottom: 2px solid #D57346; /* selection color */
    border-radius: 0px;
}

Gui--PropertyView > QTabWidget > QTabBar::tab:bottom:selected {
    background-color: rgba(30, 30, 38, 0.0);
}

QTabBar::tab:left,
QTabBar::tab:right {
    border-left-width: 4px; /* same as selected tab colored border in order to center close-button */
    border-right-width: 4px; /* same as selected tab colored border in order to center close-button */
    min-height: 14ex;
}

QTabBar::tab:right:selected {
    border-left: 4px solid #D57346; /* selection color */
    border-right-color: #1E1E26; /* same as tab content background color */
}

QTabBar::tab:left:selected {
    border-right: 4px solid #D57346; /* selection color */
    border-left-color: #1E1E26; /* same as tab content background color */
}

QTabBar::tab:!selected:hover {
    background-color: #594FF7; /* Hack for Undocked inactive tabs - can also change to #696969 */ /* was rgba(255,255,255,20) */
}

QTabBar::tab:first:selected {
    margin-left: 0; /* the first selected tab has nothing to overlap with on the left */
}

QTabBar::tab:last:selected {
    margin-right: 0; /* the last selected tab has nothing to overlap with on the right */
}

QTabBar::tab:only-one {
    margin: 0; /* if there is only one tab, we don't want overlapping margins */
}

/* Workaround for QTabBars created from docked QDockWidgets which don't draw the border if not set and reset as follows: */
QTabBar {
    border-top: 1px solid #1E1E26;  /* set color for all QTabBars */
}

QDockWidget QTabBar {
    border-color: transparent; /* set color for all QTabBars but ones created from QDockWidget */
}

QDialog QTabBar {
    border-color: transparent; /* set color for QTabBars inside Preferences dialog */
}
/* end fix */

/* ========== OPEN FILES TABS ========== */
QMdiArea > QTabBar::tab:bottom:selected { 
    background-color: #34353c;
    border-radius: 0px;
}

QMdiArea > QTabBar::tab:bottom:!selected { 
}

QMdiArea > QTabBar {
    max-height: 24px;
}
/* ========== PREFERENCE PANEL ========== */
/* QDialog#Gui__Dialog__DlgPreferences QCheckBox::indicator {
    background-color: #14141A;
}

QDialog#Gui__Dialog__DlgPreferences QCheckBox::indicator:checked {
    background-color: #D57346; QRadioButton has the same color
    image:url(qss:images_dark-light/checkbox_light.svg);
} */
/* unique styles for sections inside Preferences */

QDialog#Gui__Dialog__DlgPreferences > QListView::item {
    border-radius: 1px;
    border-left: 2px solid transparent;
    padding-bottom: 2px;
    padding-top: 2px;
    max-width: 100px;
    min-width: 100px;
}

QDialog#Gui__Dialog__DlgPreferences > QListView::icon {
    padding-left: 10px;
}

QDialog#Gui__Dialog__DlgPreferences > QListView::item:selected {
    background: #1E1E26;
    border-radius: 0px;
    border-left: 2px solid #D57346;
    margin-bottom: 1px;
}

QDialog#Gui__Dialog__DlgPreferences > QListView::item:!selected {
    background: #27282E;
    border-radius: 0px;
    border-left: 2px solid transparent;
    border-top-left-radius: 16px;
    border-top-right-radius: 0px;
    border-bottom-left-radius: 16px;
    border-bottom-right-radius: 0px;
}

QDialog#Gui__Dialog__DlgPreferences > QListView::item:!selected:hover {
    background-color: #594FF7;
    border-top-left-radius: 16px;
    border-top-right-radius: 0px;
    border-bottom-left-radius: 16px;
    border-bottom-right-radius: 0px;
}

QDialog#Gui__Dialog__DlgPreferences > QListView {
    background-color: #1E1E26;
}

/* hack to access Preference TabBar background */
QDialog#Gui__Dialog__DlgPreferences > QFrame QFrame {
    background-color: transparent; /* main background color (in Windows is #1E1E26) */
}

/* fix for previous hack that broke QTabWidget background on Windows */
QDialog#Gui__Dialog__DlgPreferences QTabWidget::pane {
    background-color: transparent; /* temporal (transparent background) */
}

QGroupBox#groupBoxNaviCube:title {
    padding-left: 5px;
}

Gui--Dialog--DlgSettingsLazyLoadedImp > QTableWidget {
    background-color: #1E1E26;
}

QDialog#Gui__Dialog__DlgPreferences QHeaderView {
    max-height: 25px;
}

/*==================================================================================================
Tab bar buttons
==================================================================================================*/
/* Close button */
QTabBar::close-button {
    subcontrol-origin: margin;
    subcontrol-position: center right; /* only works for Qt 4.6 and newer */;
    border-radius: 1px;
    background-image: url(qss:images_dark-light/close_light.svg);
    background-position: center center;
    background-repeat: none;
}

QTabBar::close-button:hover {
    background-color: rgba(255,255,255,20);
}

QTabBar::close-button:pressed {
    background-color: rgba(255,255,255,30);
}

/* Fix for lists inside Model tab */
QDockWidget QTreeView,
QDockWidget QListView,
QDockWidget QTableView {
    margin: 6px;
    min-height: 40px; /* necessary in some areas of FreeCAD */
    background-color: #1E1E26;
}

/* Buttons to scroll tabs if there is not space to show all of them: */
QTabBar::scroller {
    width: 20px; /* the width of the scroll buttons */
}

QTabBar QToolButton,
QTabBar QToolButton:hover {
    background-color: #1E1E26; /* same as main background color */
}

QTabBar QToolButton::right-arrow:enabled {
     image: url(qss:images_dark-light/right_arrow_dark.svg);
}

QTabBar QToolButton::right-arrow:disabled,
QTabBar QToolButton::right-arrow:off {
     image: url(qss:images_dark-light/right_arrow_disabled_dark.svg);
}

QTabBar QToolButton::right-arrow:hover {
     image: url(qss:images_dark-light/right_arrow_darker.svg);
}

 QTabBar QToolButton::left-arrow:enabled {
     image: url(qss:images_dark-light/left_arrow_dark.svg);
}

 QTabBar QToolButton::left-arrow:disabled,
 QTabBar QToolButton::left-arrow:off {
     image: url(qss:images_dark-light/left_arrow_disabled_dark.svg);
}

 QTabBar QToolButton::left-arrow:hover {
     image: url(qss:images_dark-light/left_arrow_darker.svg);
}

 QTabBar QToolButton::up-arrow:enabled {
     image: url(qss:images_dark-light/up_arrow_dark.svg);
}

 QTabBar QToolButton::up-arrow:disabled,
 QTabBar QToolButton::up-arrow:off {
     image: url(qss:images_dark-light/up_arrow_disabled_dark.svg);
}

 QTabBar QToolButton::up-arrow:hover {
     image: url(qss:images_dark-light/up_arrow_darker.svg);
}

 QTabBar QToolButton::down-arrow:enabled {
     image: url(qss:images_dark-light/down_arrow_light.svg);
}

 QTabBar QToolButton::down-arrow:disabled,
 QTabBar QToolButton::down-arrow:off {
     image: url(qss:images_dark-light/down_arrow_disabled_light.svg);
}

 QTabBar QToolButton::down-arrow:hover {
     image: url(qss:images_dark-light/down_arrow_lighter.svg);
}

QTabBar::tear {
    /* default OS tear better */
}


/*==================================================================================================
Tree and list views
==================================================================================================*/
QTreeView,
QListView,
QTableView {
    color: #d2d2d2;
    background-color: #14141A; /*Also controls Default Spreadsheet background  */
    alternate-background-color: rgba(20, 20, 26, 0.4) ; /* related with QListView background  */
    border-radius: 1px;
    selection-background-color: #D57346;
    selection-color: #ffffff;
}

QTableList:hover,
QTableList::item:hover {
    background-color: #ffffff;
}

QListView::item,
QTableView::item,
QTreeView::item {
    border-radius: 8px;
    margin: 1px;
    min-width:100px;

}

QListView::item:hover,
QTreeView::item:hover  {
    background-color: #594FF7;
    color: #ffffff;
}

QListView::item:selected,
QTreeView::item:selected  {
    color: #ffffff; /* should be similar to QListView selection-color */
    background-color: #D57346; /* should be similar to QListView selection-background-color */
}

/*===============================================
Property Editor QTreeView (FreeCAD custom widget)
=================================================*/

Gui--PropertyEditor--PropertyEditor QComboBox,
Gui--PropertyEditor--PropertyEditor QComboBox:editable,
Gui--PropertyEditor--PropertyEditor QComboBox:!editable,
Gui--PropertyEditor--PropertyEditor QLineEdit,
Gui--PropertyEditor--PropertyEditor QTextEdit,
Gui--PropertyEditor--PropertyEditor QPlainTextEdit,
Gui--PropertyEditor--PropertyEditor QAbstractSpinBox,
Gui--PropertyEditor--PropertyEditor QDateEdit,
Gui--PropertyEditor--PropertyEditor QDateTimeEdit {
  background-color: rgba(20, 20, 20, 1);
  border: 0px;
  border-top-left-radius: 0px;
  border-bottom-left-radius: 0px;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
  padding-left: 3px;
}

Gui--PropertyEditor--PropertyEditor {
    gridline-color: transparent; /* same as Group header background */
    font-size: 12px;
    background-color: #14141A;
    qproperty-groupTextColor: #E6E6E6;
    qproperty-groupBackground: rgba(30, 30, 38, 0.0);
}

/* fix for column items background when a link is present */
Gui--PropertyEditor--PropertyEditor > QWidget > QFrame:focus {
    background-color: #14141A; /* same as focused background color */
}

/* hack to hide weird redundant information inside the value of a Placement cell */
Gui--PropertyEditor--PropertyEditor > QWidget > QWidget > QLabel,
Gui--PropertyEditor--PropertyEditor > QWidget > QWidget > QLabel:disabled {
    color: transparent;
    background-color: transparent;
    border: none;
    border-radius: 1px;
    margin: 0px;
    padding: 0px;
}

/* hack to hide non editable cells inside Property values */
Gui--PropertyEditor--PropertyEditor QLineEdit:read-only,
Gui--PropertyEditor--PropertyEditor QLineEdit:disabled,
Gui--PropertyEditor--PropertyEditor QAbstractSpinBox:read-only,
Gui--PropertyEditor--PropertyEditor QAbstractSpinBox:disabled {
    color: transparent;
    border-color: transparent;
    background-color: transparent;
    selection-color: transparent;
    selection-background-color: transparent;
}

Gui--PropertyEditor--PropertyEditor > QWidget > QWidget > QLabel {
	color: #676773;
	background-color: #14141A; /* same as focused background color */
    padding-left: 1px;
}

/* hack to disable margin inside Property values to following elements */
Gui--PropertyEditor--PropertyEditor QSpinBox,
Gui--PropertyEditor--PropertyEditor QDoubleSpinBox,
Gui--PropertyEditor--PropertyEditor QAbstractSpinBox,
Gui--PropertyEditor--PropertyEditor QLineEdit,
Gui--PropertyEditor--PropertyEditor QComboBox {
    margin-left: 0px;
    margin-right: 0px;
    padding-top: 0px;
    padding-bottom: 0px;
}

/* reset min-height to 0px inside list views */
QTreeView > QWidget > QComboBox,
QTreeView > QWidget > QAbstractSpinBox,
QTreeView > QWidget > QSpinBox,
QTreeView > QWidget > QDoubleSpinBox,
QTreeView > QWidget > QLineEdit,
QTreeView > QWidget > QTextEdit,
QTreeView > QWidget > QTimeEdit,
QTreeView > QWidget > QDateEdit,
QTreeView > QWidget > QDateTimeEdit,
QTreeView > QWidget > Gui--ColorButton {
    min-height: 0px;
    background-color: transparent;
}

/* set border-radius to 0px inside list views */
QTreeView > QWidget > QComboBox,
QTreeView > QWidget > QAbstractSpinBox,
QTreeView > QWidget > QSpinBox,
QTreeView > QWidget > QDoubleSpinBox,
QTreeView > QWidget > QLineEdit,
QTreeView > QWidget > QTextEdit,
