/*==================================================================================================
Branch system for QTreeViews
==================================================================================================*/

QTreeView::branch  {
    background: transparent;
}

QTreeView::branch:has-siblings:!adjoins-item  {
    background-image: url(qss:midnight_icons/branch_vline_light.svg);
    background-position: center; /* Center the image */
    background-repeat: no-repeat; /* Do not repeat the image */
}

QTreeView::branch:has-siblings:adjoins-item  {
    background-image: url(qss:midnight_icons/branch_more_light.svg);
    background-position: center; /* Center the image */
    background-repeat: no-repeat; /* Do not repeat the image */
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item  {
    background-image: url(qss:midnight_icons/branch_end_light.svg);
    background-position: center; /* Center the image */
    background-repeat: no-repeat; /* Do not repeat the image */
}

QTreeView::branch:closed:has-children:has-siblings  {
    background-image: url(qss:midnight_icons/branch_more_closed_light.svg);
    background-position: center; /* Center the image */
    background-repeat: no-repeat; /* Do not repeat the image */
}

QTreeView::branch:has-children:!has-siblings:closed  {
    background-image: url(qss:midnight_icons/branch_end_closed_light.svg);
    background-position: center; /* Center the image */
    background-repeat: no-repeat; /* Do not repeat the image */
}

QTreeView::branch:open:has-children:has-siblings  {
    background-image: url(qss:midnight_icons/branch_more_open_light.svg);
    background-position: center; /* Center the image */
    background-repeat: no-repeat; /* Do not repeat the image */
}

QTreeView::branch:open:has-children:!has-siblings  {
    background-image: url(qss:midnight_icons/branch_end_open_light.svg);
    background-position: center; /* Center the image */
    background-repeat: no-repeat; /* Do not repeat the image */
}

